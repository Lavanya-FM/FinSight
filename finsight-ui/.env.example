# Environment Configuration Template
# Copy this file to .env and update the values for your environment

# Node Environment (development, production, test)
NODE_ENV=development

# Supabase Configuration (Required for both local and cloud)
REACT_APP_SUPABASE_URL=your-supabase-url-here
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# API Configuration (Required for cloud deployment)
# For local development: http://localhost:8000
# For cloud deployment: your-backend-api-url
REACT_APP_API_URL=http://localhost:8000

# Build Configuration
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true

# Optional: For production builds
# REACT_APP_ENVIRONMENT=production
# REACT_APP_VERSION=1.0.0

# Deployment URLs (for reference - uncomment and update as needed)
# REACT_APP_API_URL=https://your-backend-api.herokuapp.com
# REACT_APP_API_URL=https://your-backend-api.vercel.app
# REACT_APP_API_URL=https://your-backend-api.netlify.app
# REACT_APP_API_URL=https://your-backend-api.onrender.com
